<h1 class="mt-4">Game</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<form action="/deleteGame" method="post">
    <div class="input">
        <label for="gameName">name</label>
        <input type="text" id="gameName" name="gameName">
    </div>
    <button type="submit">DELETE</button>
</form>

<table class="table table-hover">
       
    <thead>
      <tr>
        <th scope="col">gameName</th>
        <th scope="col">gameDetail</th>
        <th scope="col">gameProducer</th>
        <th scope="col">company</th>
        <th scope="col">ignScore</th>
        <th scope="col">userID</th>
      </tr>
    </thead>
   
    <tbody>
            <% games.forEach(function (game) { %>
    
                
            
            <tr class="table-danger">
                <td> <%= game.gameName %></td>
                <td><%= game.gameDetail %></td>
                <td><%= game.gameProducer %></td>
                <td><%= game.company %></td>
                <td><%= game.ignScore %></td>
                <td><%= game.userID %></td>
                <td><form action="/updateGame" method="POST">
                    <input type="hidden" id="gameName123" name="gameName123"  value=<%= game.gameName %>>
                    <input type="submit" value="Go to Google" />
                </form></td>
            </tr>
            <% }) %>
    </tbody>
</table> 







